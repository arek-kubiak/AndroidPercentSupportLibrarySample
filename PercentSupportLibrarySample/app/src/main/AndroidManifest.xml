<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.akubiak.samples.percentsupportlibrarysample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CodeBehindSampleActivity" />
        <activity android:name=".BasicUsageSampleLinearLayoutActivity" />
        <activity android:name=".BasicUsageSamplePercentRelativeLayoutActivity" />
        <activity android:name=".MainSampleLinearLayoutActivity" />
        <activity android:name=".MainSamplePercentRelativeLayoutActivity" />
        <activity android:name=".MainSamplePercentFrameLayoutActivity" />
        <activity android:name=".LayoutAspectRatioSampleActivity"/>
    </application>

</manifest>